#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/israfil/flowentech/saas/flowtim/flowtim-frontend/node_modules/loose-envify/node_modules:/Users/israfil/flowentech/saas/flowtim/flowtim-frontend/node_modules:/Users/israfil/flowentech/saas/flowtim/node_modules:/Users/israfil/flowentech/saas/node_modules:/Users/israfil/flowentech/node_modules:/Users/israfil/node_modules:/Users/node_modules:/node_modules:/Users/israfil/flowentech/saas/flowtim/flowtim-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/israfil/flowentech/saas/flowtim/flowtim-frontend/node_modules/loose-envify/node_modules:/Users/israfil/flowentech/saas/flowtim/flowtim-frontend/node_modules:/Users/israfil/flowentech/saas/flowtim/node_modules:/Users/israfil/flowentech/saas/node_modules:/Users/israfil/flowentech/node_modules:/Users/israfil/node_modules:/Users/node_modules:/node_modules:/Users/israfil/flowentech/saas/flowtim/flowtim-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
